    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/equal_bg"
    tools:context=".ColorEqualizeMain">


    <TextView
        android:id="@+id/memcombo2"
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:fontFamily="@font/omori_font"
        android:gravity="top|center"
        android:textColor="#FF071C"
        android:text="sssss"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/score_bar"

        />

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/streak"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_marginLeft="-30dp"
        android:layout_marginTop="-20dp"
        android:rotation="20"
        android:src="@drawable/treak2_unscreen"
        android:visibility="invisible"
        app:layout_constraintLeft_toRightOf="@id/memcombo2"
        app:layout_constraintTop_toBottomOf="@id/score_bar"

        />

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/streak2"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_marginTop="-20dp"
        android:layout_marginRight="-30dp"
        android:rotation="-20"
        android:src="@drawable/treak2_unscreen"
        android:visibility="invisible"
        app:layout_constraintEnd_toStartOf="@+id/memcombo2"
        app:layout_constraintTop_toBottomOf="@id/score_bar" />

    <LinearLayout
        android:id="@+id/score_bar"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="-20dp"
        android:orientation="horizontal"
        android:weightSum="1"
        app:layout_constraintBottom_toTopOf="@id/equal_frame"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.gridlayout.widget.GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.2"
            app:columnCount="2"
            app:rowCount="1">

            <ImageView
                android:layout_width="40dp"
                android:layout_height="70dp"
                android:padding="5dp"
                android:src="@drawable/timer" />

            <TextView
                android:id="@+id/memtimer"
                android:layout_width="50dp"
                android:layout_height="70dp"
                android:fontFamily="@font/omori_font"
                android:gravity="center_vertical"
                android:text="0"
                android:textColor="@color/white"
                android:textSize="28dp" />
        </androidx.gridlayout.widget.GridLayout>

        <TextView
            android:id="@+id/mem_score"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_weight="0.6"
            android:fontFamily="@font/omori_font"
            android:gravity="top|center"
            android:text="0"
            android:textColor="@color/white"
            android:textSize="60dp" />

        <androidx.gridlayout.widget.GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:layout_weight="0.2"
            app:columnCount="2"
            app:rowCount="1">

            <TextView
                android:id="@+id/memround"
                android:layout_width="50dp"
                android:layout_height="70dp"
                android:fontFamily="@font/omori_font"
                android:gravity="right|center_vertical"
                android:text="0"
                android:textColor="@color/white"
                android:textSize="28dp"
                app:layout_columnWeight="1"
                app:layout_gravity="end" />

            <ImageView
                android:layout_width="30dp"
                android:layout_height="50dp"
                android:layout_margin="6dp"
                android:src="@drawable/round2"
                app:layout_gravity="center" />
        </androidx.gridlayout.widget.GridLayout>

    </LinearLayout>

